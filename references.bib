	pmid = {35730170},
	keywords = {Blindness, Blood Glucose, Diabetes Mellitus, diabetic macular edema, Diabetic retinopathy, Diabetic Retinopathy, Humans, risk factors, Risk Factors, screening, visual loss},
	pages = {235--243},
	file = {Full Text:/Users/rolo/Zotero/storage/SVLHE9KH/Scanlon - 2022 - Improving the screening of risk factors in diabeti.pdf:application/pdf},
}


