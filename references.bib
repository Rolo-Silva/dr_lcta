@article{trento_quality_2013,
	title = {Quality of life, impaired vision and social role in people with diabetes: a multicenter observational study},
	volume = {50},
	issn = {1432-5233},
	shorttitle = {Quality of life, impaired vision and social role in people with diabetes},
	url = {https://doi.org/10.1007/s00592-013-0470-1},
	doi = {10.1007/s00592-013-0470-1},
	abstract = {Diabetic retinopathy may induce visual impairment. We evaluated vision-related quality of life in patients with visual acuity {\textless}5/10 in the better eye induced by retinopathy using the 25-item National Eye Institute Visual Functioning Questionnaire (NEI VFQ-25). The NEI VFQ-25 was self-administered to 196 patients in 3 Italian centres (A, B and C; n = 64, 61 and 71, respectively) dedicated to DR screening and treatment. Patients in the 3 centres did not differ by age, gender, occupation and diabetes duration. Multivariate analysis demonstrated that reduced visual acuity was associated with decreased scores for General Vision, Near Activities, Distance Activities, Visual-Specific Social Functioning, Mental Health, Role Difficulties and Dependency, Driving, Colour Vision and Peripheral Vision (p {\textless} 0.01, all). Treatment by photocoagulation was associated with reduced scores in General Health (−8.3; p = 0.002), General Vision (−7.2; p = 0.001), Visual-Specific Role Difficulties (−8.8; p = 0.015) and Driving (−13.7; p = 0.003). Centre affiliation was associated with different scores for General Health, Ocular pain, Distance Activities, Visual-Specific Social Functioning and Role Difficulties and Peripheral Vision. Women had higher scores for General Vision (p = 0.015), Near Activities (p = 0.005), Distance Activities (p = 0.006), Visual-Specific Social Functioning (p = 0.03), Visual-Specific Mental Health (p = 0.035) and Colour Vision (p = 0.012). Diabetic retinopathy and vision loss modify the way people perceive their own ability to function autonomously. More data should be collected to confirm this interpretation and to guide the development of more appropriate settings to improve approach and support to patients.},
	language = {en},
	number = {6},
	urldate = {2023-10-13},
	journal = {Acta Diabetologica},
	author = {Trento, Marina and Passera, Pietro and Trevisan, Martina and Schellino, Francesca and Sitia, Elena and Albani, Stefano and Montanaro, Marcello and Bandello, Francesco and Scoccianti, Lucia and Charrier, Lorena and Cavallo, Franco and Porta, Massimo},
	month = dec,
	year = {2013},
	keywords = {Diabetic retinopathy, Quality of life, Vision-related quality of life, Visual loss},
	pages = {873--877},
	file = {Full Text PDF:/Users/rolo/Zotero/storage/NUHP8JGT/Trento et al. - 2013 - Quality of life, impaired vision and social role i.pdf:application/pdf},
}


